{
  "name": "star-orgs",
  "version": "0.0.0",
  "description": "Azure Active Directory Organization Chart Viewer",
  "main": "server.js",
  "keywords": [
    "azure",
    "active directory",
    "ad"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ritterim/star-orgs"
  },
  "scripts": {
    "audit": "nsp check package",
    "build": "webpack -d",
    "check": "npm run audit && npm outdated --depth 0",
    "coverage": "nyc --reporter=lcov --reporter=text --reporter=html npm test",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "lint": "eslint src test",
    "start": "npm run build && node ./server.js",
    "start:watch": "concurrently \"npm run watch\" \"node ./server.js\"",
    "test": "ava --require babel-core/register",
    "validate": "npm run lint && npm test",
    "prepare": "npm run build && npm run validate && npm run check",
    "pp": "npm run prepare",
    "major": "pmm major",
    "minor": "pmm minor",
    "patch": "pmm patch",
    "watch": "webpack -d --watch"
  },
  "author": {
    "name": "Ritter Insurance Marketing"
  },
  "pre-commit": [
    "validate"
  ],
  "dependencies": {
    "compression": "^1.6.2",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "request-promise": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-function-bind": "^6.5.2",
    "babel-preset-es2015": "^6.9.0",
    "concurrently": "^2.1.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.5.1",
    "fetch-mock": "^4.5.1",
    "nsp": "^2.2.0",
    "nyc": "^6.1.1",
    "pmm": "^1.3.0",
    "pre-commit": "^1.1.2",
    "webpack": "^1.13.1"
  }
}
